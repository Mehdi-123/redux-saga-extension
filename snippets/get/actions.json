{
  "Generate GET actions": {
    "prefix": "getActions",
    "body": [
      "export function get${1:Feature}Action(params: any) {",
      "  return {",
      "    type: GET_${2:FEATURE},",
      "    params,",
      "  };",
      "}",
      "export function get${1:Feature}SuccessAction(data: any) {",
      "  return {",
      "    type: GET_${2:FEATURE}_SUCCESS,",
      "    data,",
      "  };",
      "}",
      "export function get${1:Feature}ErrorAction(error: any) {",
      "  return {",
      "    type: GET_${2:FEATURE}_ERROR,",
      "    error,",
      "  };",
      "}",
      "export function cleanGet${1:Feature}Action() {",
      "  return {",
      "    type: CLEAN_GET_${2:FEATURE},",
      "  };",
      "}"
    ],
    "description": "Generate GET actions content for a given feature"
  }
}
