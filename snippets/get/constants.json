{
  "Generate GET constants": {
    "prefix": "getConstants",
    "body": [
      "export const GET_${1:FEATURE} = 'app/${3:path}/GET_${1:FEATURE}';",
      "export const GET_${1:FEATURE}_SUCCESS = 'app/${3:path}/GET_${1:FEATURE}_SUCCESS';",
      "export const GET_${1:FEATURE}_ERROR = 'app/${3:path}/GET_${1:FEATURE}_ERROR';",
      "export const CLEAN_GET_${1:FEATURE} = 'app/${3:path}/CLEAN_GET_${1:FEATURE}';"
    ],
    "description": "Generate GET constants content for a given feature"
  }
}
